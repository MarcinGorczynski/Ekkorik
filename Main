using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace ConsoleApp1
{
    class Program
    {
        static void Main(string[] args)
        {
            Console.WriteLine("podaj swoje imie"); //prosi o podanie imienia 
            string imie = Console.ReadLine(); // tu użytkownik wpisuje z klawiatury imie
            Console.WriteLine("Cześć " + imie); // tu jest stała cześć + imie które zostało wprowadzone

            Console.WriteLine("podaj A"); // prosi o podanie A
            int a = int.Parse(Console.ReadLine()); //
            Console.WriteLine("podaj B"); // prosi o podanie B
            int b = int.Parse(Console.ReadLine()); //
            int w = a + b;

            Console.WriteLine(a + " + " + b + " = " + w);
            Console.WriteLine(a + b + w);

            Console.ReadKey();
        }
    }
}
